{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Elasticsearch": {
      "BypassCertificateValidation": true
    }
  },
  "Kafka": {
    "Producer": {
      "BootstrapServers": "localhost:9092"
    },
    "Consumer": {
      "AllowAutoCreateTopics": true,
      "BootstrapServers": "localhost:9092"
    }
  }
}
